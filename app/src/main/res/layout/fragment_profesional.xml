<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Profesional">

    <LinearLayout
        android:layout_width="409dp"
        android:layout_height="729dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="1dp">

        <Button
            android:id="@+id/btnNuevaCuenta"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Nueva cuenta" />

        <Button
            android:id="@+id/btnGenQR"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="GENERAR QR" />

        <Button
            android:id="@+id/btnCarta"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="CARTA" />

        <Button
            android:id="@+id/btnRestaurante"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="RESTAURANTES" />

        <Button
            android:id="@+id/btnAlergenos"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="ALERGENOS" />
            android:id="@+id/btnGestionComandas"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="GESTIÃ“N DE COMANDAS" />

        <ImageView
            android:id="@+id/imageQR"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            tools:srcCompat="@tools:sample/avatars" />

        <FrameLayout
            android:id="@+id/frameLayout1"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        </FrameLayout>
    </LinearLayout>

</FrameLayout>